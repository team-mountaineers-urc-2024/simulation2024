    <!-- Base Joint -->
    <joint name="base_head_joint" type="fixed">
        <origin xyz="0.0 0.0 0.3225" rpy="1.57079632679 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="mallet_head"/>
