        <visual name="mallet_ball_visual">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.030 0.037 0.030"/>
            </geometry>
            <material name="">
                <color rgba="1.0 0.15 0.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
